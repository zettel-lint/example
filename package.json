{
  "name": "example-zettel-lint",
  "version": "0.9.2",
  "description": "Example repo demonstrating zettel linting",
  "scripts": {
    "zl-references": "zettel-lint index --path . --ignore-dirs \"./node_modules/**\" --reference-file references.md --show-orphans",
    "zl-import-trello": "zettel-lint import --verbose --source trello --output-folder . --path trello-board.json",
    "zl": "zettel-lint --help",
    "zl-version": "zettel-lint --version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zettel-lint/example.git"
  },
  "keywords": [
    "zettel",
    "lint",
    "javascript"
  ],
  "author": "Craig Nicol",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zettel-lint/example/issues"
  },
  "homepage": "https://github.com/zettel-lint/example#readme",
  "dependencies": {},
  "devDependencies": {
    "zettel-lint": "^0.9.2"
  }
}
